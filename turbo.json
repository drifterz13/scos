{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": []
    },
    "lint": {
      "dependsOn": []
    },
    "format": {
      "dependsOn": []
    },
    "ci": {
      "dependsOn": []
    },
    "test:unit": {
      "dependsOn": []
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
